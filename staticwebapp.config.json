{
	"routes": [
	{
      		"route": "/*",
      		"rewrite": "/index.html"
    	},
        {
	      "route": "/images/*",
	      "headers": {
		"cache-control": "must-revalidate, max-age=15770000"
	      }
	}	
	],
	"navigationFallback": {
	"rewrite": "/index.html",
	"exclude": ["/images/*.{png,jpg,gif}", "/static/js/*", "/static/css/*", "*.json"]
	},
	"mimeTypes": {
	".json": "text/json",
	".js": "text/javascript",
	".css": "text/css",
	".jpg": "image/jpeg"
	}
}
