{
		"routes": [
      {
      "route": "/*",
      "rewrite": "/index.html"
      }
		],
		"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/static/js/*", "/images/*", "/static/css/*", "*.json"]
		},
		"mimeTypes": {
		".json": "text/json",
		".js": "text/javascript",
		".css": "text/css"
		}
}
